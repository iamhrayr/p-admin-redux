{
  "compilerOptions": {
    "outDir": "./dist/",
    "baseUrl": "./",
    "paths": {
      "Views/*": ["src/views/*"],
      "Components/*": ["src/components/*"],
      "Utils/*": ["src/utils/*"],
      "Store/*": ["src/store/*"]
    },
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "esModuleInterop": true
  },
  "include": ["./src/**/*"]
}
